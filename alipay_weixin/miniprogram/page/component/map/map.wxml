<view  class="page">
  <view  class="page-section">
    <view  class="page-section-demo">
      <onekit-map  id="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="{{scale}}" controls="{{controls}}" bindcontrolTap="controltap" markers="{{markers}}" bindmarkerTap="markertap" polyline="{{polyline}}" polygon="{{polygon}}" circles="{{circles}}" bindregionChange="regionchange" bindtap="tap" bindcalloutTap="callouttap" show-location style="width: 100%; height: 200px;" include-points="{{includePoints}}" ground-overlays="{{ground-overlays}}">
      </onekit-map>
    </view>
  </view>
  <view  class="page-section-btns">
    <view  bindtap="demoResetMap">恢复</view>
    <view  bindtap="demoGetCenterLocation">获取中心点坐标</view>
    <view  bindtap="demoMoveToLocation">回到定位点</view>
  </view>
  <view  class="page-section-btns">
    <view  bindtap="demoMarkerAnimation">Marker 动画</view>
    <view  bindtap="demoMarkerLabel">Label</view>
    <view  bindtap="demoMarkerCustomCallout">CustomCallout</view>
  </view>
  <view  class="page-section-btns">
    <view  bindtap="demoMarkerAppendStr">文字Marker</view>
    <view  bindtap="demoTrafficOverlay">路况展示</view>
    <view  bindtap="demoShowRoute">步行路线规划</view>
  </view>
  <view  class="page-section-btns">
    <view  bindtap="demoCompass">指南针</view>
    <view  bindtap="demoScale">比例尺</view>
    <view  bindtap="demoGesture">手势</view>
  </view>
  <view  class="page-section-btns">
    <view  bindtap="demoPolyline">线</view>
    <view  bindtap="demoPolygon">多边形</view>
    <view  bindtap="demoCircle">圆</view>
  </view>
  <view  class="page-section-btns">
  </view>
</view>
